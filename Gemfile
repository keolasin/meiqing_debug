source "https://rubygems.org"

gem 'tzinfo-data'
gem 'webrick' # webrick no longer bundled gem in ruby 3.0, need to explicitly include for jekyll to work

gem "github-pages", group: :jekyll_plugins

gem "wdm", "~> 0.1.0" if Gem.win_platform?

group :jekyll_plugins do
    gem "jekyll-feed"
    gem 'jekyll-sitemap'
    gem 'hawkins'
end